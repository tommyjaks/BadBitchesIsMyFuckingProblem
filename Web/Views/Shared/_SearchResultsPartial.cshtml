@model Web.Models.SearchModel

@{
    Layout = "~/Views/Shared/_Layout2.cshtml";
}
<div id="loginForm">
    @if (Model.Usernames.Count == 0)
    {
        <h3 class="text-error">No items matched your search query!</h3>
    }
    else
    {
        foreach (var result in Model.Usernames)
        {
            <a href='@(Url.Action("UserProfile", "UserHome", new { username = @result.Username }))'>
                @result.Firstname  &nbsp;@result.Lastname
            </a>
              
        }
    }
</div>
